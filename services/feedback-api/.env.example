# Feedback API Configuration
PORT=4001
DB_PATH=./data/feedback.db

# API Keys
# WRITE key: used by the frontend to submit feedback (safe to expose in config)
# ADMIN key: used to read/query feedback data (keep secret)
API_KEY_WRITE=changeme-write
API_KEY_ADMIN=changeme-admin

# CORS: origin of the OpenDataPlanner frontend
CORS_ORIGIN=http://localhost:4000
